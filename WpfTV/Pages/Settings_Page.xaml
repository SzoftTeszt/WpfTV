<UserControl x:Class="WpfTV.Pages.Settings_Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfTV.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="0,40" Orientation="Horizontal" VerticalAlignment="Stretch" HorizontalAlignment="Center">
        <local:Element Title="Network Setting" Kind="Wifi"/>
        <local:Element Title="Time Settin" Kind="Timelapse"/>
        <local:Element Title="Common" Kind="Layers"/>
        <local:Element Title="About Us" Kind="Information"/>
    </StackPanel>
</UserControl>
